<?xml version="1.0" encoding="UTF-8"?>
<mei meiversion="2013" 
  xmlns="http://www.music-encoding.org/ns/mei" 
 >
  <!-- 
    This file serves as a template defining all possible elements and attributes editable in MerMEId. 
    Elements added using the editor interface are copied from this file.
    The template for NEW files created with MerMEId is the file named new_file.xml, which may be simplified
    to reduce the amount of empty elements when starting a new document.

    Danish Centre for Music Editing
    The Royal Library, Copenhagen 2013
  -->
  <meiHead>
    <fileDesc>
      <titleStmt>
        <title/>
      </titleStmt>
      <pubStmt>
        <respStmt>
          <resp>Publisher</resp>
          <corpName>
            <abbr/>
            <expan/>
            <address>
              <addrLine/>
              <addrLine><ptr target="" label="WWW"/></addrLine>
              <addrLine><ptr target="" label="E-mail"/></addrLine>
            </address>
          </corpName>
          <persName role="editor"/>
        </respStmt>
        <date isodate="" startdate="" enddate="" notbefore="" notafter=""/>
        <availability>
          <acqSource/>
          <accessRestrict/> 
          <useRestrict/>
        </availability>
      </pubStmt>      
      <seriesStmt>
        <title/>
        <identifier type="file_collection"/>
      </seriesStmt>
      <notesStmt>
        <annot type="private_notes"/>
      </notesStmt>
      <sourceDesc>
        <source analog="frbr:manifestation">
          <identifier label=""/>
          <titleStmt>
            <title/>
            <respStmt>
              <persName role=""/>
            </respStmt>
          </titleStmt>
          <pubStmt>
            <publisher/>
            <pubPlace/>
            <date isodate="" startdate="" enddate="" notbefore="" notafter=""/>
          </pubStmt>
          <physDesc>
            <titlePage label="">
              <p n="MerMEId_temporary_wrapper"/>
            </titlePage>
            <extent unit=""/>
            <dimensions unit=""/>
            <watermark/>
            <physMedium/>
            <plateNum/>
          </physDesc>
          <notesStmt>
            <annot type="source_description"/>
            <annot type="links">
              <ptr target="" mimetype="" label=""/>
            </annot>
          </notesStmt>
          <classification>
            <termList>
              <term classcode="DcmContentClass"/>
              <term classcode="DcmPresentationClass"/>
              <term classcode="DcmAuthorityClass"/>
              <term classcode="DcmScoringClass"/>
              <term classcode="DcmStateClass"/>
              <term classcode="DcmCompletenessClass"/>
              <term/>
            </termList>
          </classification>
          <itemList>
            <item label="" target="">
              <identifier label=""/>
              <physDesc>
                <condition/>
                <titlePage label="">
                  <p n="MerMEId_temporary_wrapper"/>
                </titlePage>
                <provenance>
                  <eventList>
                    <event   evidence="">
                      <p/>
                      <date isodate="" startdate="" enddate="" notbefore="" notafter=""/>
                      <geogName role=""/>
                      <corpName role=""/>
                      <persName role=""/>
                    </event>
                  </eventList>
                </provenance>
                <extent unit=""/>
                <dimensions unit=""/>
                <handList>
                  <hand medium="" initial="true"/>
                </handList>
                <watermark/>
                <physMedium/>
                <plateNum/>
              </physDesc>
              <physLoc>
                <repository>
                  <corpName/>
                  <geogName/>
                  <identifier authority="RISM" authURI="http://www.rism.info"/>
                  <ptr target="" mimetype="" label=""/>
                </repository>
                <identifier/>
              </physLoc>
              <notesStmt>
                <annot type="source_description"/>
                <annot type="links">
                  <ptr target="" mimetype="" label=""/>
                </annot>
              </notesStmt>
              <componentGrp>
                <item label="">
                  <physDesc/>
                </item>
              </componentGrp>
            </item>
          </itemList>
          <componentGrp>
            <source label="" target="" analog="frbr:manifestation">
              <titleStmt>
                <title/>
              </titleStmt>
              <physDesc/>
            </source> 
          </componentGrp>
          <relationList>
            <relation rel="isEmbodimentOf" target=""   label=""/>
          </relationList>
        </source>
        <source label="" target="">
          <relationList>
            <relation rel="isEmbodimentOf" target=""   label=""/>
          </relationList>
        </source>
        <source>
          <relationList>
            <relation rel="isEmbodimentOf" target=""   label=""/>
            <relation rel="hasPart" target=""   label=""/>
          </relationList>
        </source>
        <source>
          <itemList>
            <item label="">
              <physDesc/>
            </item>
          </itemList>
        </source>
      </sourceDesc>
    </fileDesc>
    
    <encodingDesc>
      <appInfo>
        <application version="2015">
          <name>MerMEId</name>
        </application>
      </appInfo>
      <projectDesc>
        <p/>
      </projectDesc>
    </encodingDesc>
    
    <workDesc>
      <work analog="frbr:work">
        <identifier label=""/>
        <titleStmt>
          <title xml:lang="en"/>
          <title type="alternative" xml:lang="en"/>
          <title type="uniform" xml:lang="en"/>
          <title type="original" xml:lang="en"/>
          <title type="subordinate" xml:lang="en"/>
          <title type="text_source" xml:lang="en"/>
          <respStmt>
            <persName role="composer"/>
          </respStmt>
        </titleStmt>        
        <history>
          <creation>
            <date isodate="" startdate="" enddate="" notbefore="" notafter=""/>
            <geogName/>            
          </creation>
          <p n="MerMEId_temporary_wrapper"/>
          <eventList type="history">
            <event evidence="">
              <date isodate="" startdate="" enddate="" notbefore="" notafter=""/>
              <p/>
            </event>
          </eventList>
          <eventList type="performances">
            <event>
              <date isodate="" startdate="" enddate="" notbefore="" notafter=""/>
              <geogName role="venue"/>
              <geogName role="place"/>
              <corpName role="ensemble"/>
              <persName role="conductor"/>
              <p n="MerMEId_temporary_wrapper"/>
              <biblList>
                <head>Reviews</head>
                <bibl label="">
                  <genre>article</genre>
                  <genre>journal</genre>
                  <author/>
                  <date isodate="" notbefore="" notafter=""/>
                  <title level="a"/>
                  <title level="j"/>
                  <editor/>
                  <biblScope unit="vol"/>
                  <biblScope unit="issue"/>
                  <biblScope unit="page"/>
                  <pubPlace/>
                  <publisher/>
                  <ptr target="" label=""/>
                </bibl>
              </biblList>
              <biblList>
                <head>Evidence</head>
              </biblList>
            </event>
          </eventList>
        </history>
        
        <langUsage>
          <language xml:id="en">English</language>
        </langUsage>
        
        <biblList>
          <head>Bibliography</head>
        </biblList>
        <biblList>
          <head/>
          <bibl label="">
            <genre/>
            <author/>
            <creation>
              <date isodate=""/>
              <geogName/>
            </creation>
            <physLoc>
              <repository>
                <corpName/>
                <geogName/>
                <identifier authority="RISM" authURI="http://www.rism.info/"/>
              </repository>
              <identifier/>
            </physLoc>
            <annot/>
            <title level="a"/>
            <title level="m"/>
            <title level="s"/>
            <title level="j"/>
            <editor/>
            <biblScope unit="vol"/>
            <biblScope unit="issue"/>
            <biblScope unit="page"/>
            <imprint>
              <date/>
              <pubPlace/>
              <publisher/>
            </imprint>
            <ptr target="" label=""/>
            <relatedItem rel="host">
              <bibl label="">
                <title/>
                <biblScope unit=""/>
              </bibl>
            </relatedItem>
          </bibl>
        </biblList>
        
        <notesStmt>
          <annot type="links">
            <ptr target="" mimetype="" label=""/> 
          </annot>
          <annot type="general_description" label=""/>
        </notesStmt>        
        
        <classification>
          <classCode xml:id="DcmContentClass"/>
          <classCode xml:id="DcmPresentationClass"/>
          <classCode xml:id="DcmAuthorityClass"/>
          <classCode xml:id="DcmScoringClass"/>
          <classCode xml:id="DcmStateClass"/>
          <classCode xml:id="DcmCompletenessClass"/>
          <termList>
            <term/>
          </termList>
        </classification>

        <expressionList>
          <expression   analog="frbr:expression" n="">
            <identifier label=""/>
            <titleStmt>
              <title xml:lang="en"/>
              <respStmt>
                <persName role=""/>
              </respStmt>
            </titleStmt>
            <incip>     
              <incipText> 
                <p xml:lang="en"/>
              </incipText>
              <graphic target="" label="" targettype="lowres"/>
              <incipCode form=""/>
            </incip>
            <key pname="" accid="" mode=""/>
            <tempo/>
            <meter count="" unit="" sym=""/>
            <history>
              <creation>
                <date isodate="" startdate="" enddate="" notbefore="" notafter=""/>
                <geogName/>            
              </creation>
              <p n="MerMEId_temporary_wrapper"/>
              <eventList type="history">
                <event>
                  <date isodate="" startdate="" enddate="" notbefore="" notafter=""/>
                  <p/>
                </event>
              </eventList>
              <eventList type="performances">
                <event>
                  <date isodate="" startdate="" enddate="" notbefore="" notafter=""/>
                  <geogName role="venue"/>
                  <geogName role="place"/>
                  <corpName role="ensemble"/>
                  <persName role="conductor"/>
                  <p n="MerMEId_temporary_wrapper"/>
                  <biblList>
                    <head>Reviews</head>
                    <bibl   label="">
                      <genre>article</genre>
                      <genre>journal</genre>
                      <author/>
                      <date isodate="" notbefore="" notafter=""/>
                      <title level="a"/>
                      <title level="j"/>
                      <editor/>
                      <biblScope unit="vol"/>
                      <biblScope unit="issue"/>
                      <biblScope unit="page"/>
                      <pubPlace/>
                      <publisher/>
                      <ptr target="" label=""/>
                    </bibl>
                  </biblList>
                  <biblList>
                    <head>Evidence</head>
                  </biblList>
                </event>
              </eventList>
            </history>
            <perfMedium analog="marc:048" authority="marcmusperf">
              <castList>
                <castItem   n="">
                  <instrVoice count="" solo="" code="" n=""/> 
                  <role>
                    <name  xml:lang="en"/>
                  </role>
                  <roleDesc xml:lang="en"/>
                </castItem>
              </castList>
              <instrumentation>
                <instrVoiceGrp code="" n="">
                  <head/>
                  <instrVoice code="" count="1" solo="false" n=""/>
                </instrVoiceGrp>
                <instrVoice code="" count="1" solo="false" n=""/>
              </instrumentation>
            </perfMedium>
            <extent unit=""/>
            <notesStmt>
              <annot/>
            </notesStmt>
            <classification>
              <termList>
                <term/>
              </termList>
            </classification>
            
            <!-- top-level components -->            
            <componentGrp>
              <expression   n=""> 
                <titleStmt>
                  <title xml:lang="en"/>
                  <respStmt>
                    <persName role=""/> 
                  </respStmt>
                </titleStmt>
                <incip>     
                  <incipText> 
                    <p xml:lang="en"/>
                  </incipText>
                  <graphic target="" label="" targettype="lowres"/>
                  <graphic target="" label="" targettype="hires"/>
                  <incipCode form=""/>
                </incip>
                <key pname="" accid="" mode=""/>
                <tempo/>
                <meter count="" unit="" sym=""/>
                <perfMedium analog="marc:048" authority="marcmusperf">
                  <castList>
                    <castItem/>
                  </castList>
                  <instrumentation/>
                </perfMedium>
                <extent unit=""/>
                <notesStmt>
                  <annot/>
                </notesStmt>
                
                <!-- next sub-level components -->
                <componentGrp/>
                <relationList>
                  <relation rel="hasReproduction" label="" target=""/> 
                </relationList>
              </expression>
            </componentGrp>
            <relationList>
              <relation rel="hasReproduction" label="" target=""/> 
            </relationList>
          </expression>
        </expressionList>
        <relationList>
          <relation rel="isPartOf" label="" target=""/> 
        </relationList>
      </work>
    </workDesc>
    
    <revisionDesc>
      <change   isodate="" resp="">
        <changeDesc>
          <p/>
        </changeDesc>
      </change>
    </revisionDesc>
    
  </meiHead>

  <music/>
  
</mei>
